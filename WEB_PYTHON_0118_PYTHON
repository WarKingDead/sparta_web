                import requests
                from bs4 import BeautifulSoup

                headers = {'User-Agent' : 'Mozilla/5.0 (Windows NT 10.0; Win64; x64)AppleWebKit/537.36 (KHTML, like Gecko) Chrome/73.0.3683.86 Safari/537.36'}
                data = requests.get('https://movie.naver.com/movie/sdb/rank/rmovie.naver?sel=pnt&date=20210829',headers=headers)

                soup = BeautifulSoup(data.text, 'html.parser')

                movies = soup.select('#old_content > table > tbody > tr')

                for movie in movies:
                    fuxxtitle = movie.select_one('td.title > div > a')
                    if  fuxxtitle is not None:
                        title = fuxxtitle.text
                        rank = movie.select_one('td:nth-child(1) > img')['alt']  <-['alt']ÏùÑ Ïó¨Í∏∞Îã§ ÎÑ£Í≥†
                         print(rank) <- Î°ú ÎëêÎäî Ïù¥Ïú†Îäî
                        print(rank['alt']) <-Ïù¥Îî∞Í∞Ä print()Î•º Îòê ÏßÄÏö∞Í≥† Îî¥Í±∞ Ïì∞Í≥† ÏßÄÎûÑÌï† Í±∞ÎãàÍπå ['alt']Î•º ÏúÑÎ°ú Î∂ôÏù¥Îäî Í±∞.


